{
  "expo": {
    "name": "generate-website",
    "slug": "Magical-first-website",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "magicalfirstwebsite",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/generate-website.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Magicalfirstwebsite",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/generate-website.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Magicalfirstwebsite"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/generate-website.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d5764931-743a-4a1f-9c32-2c5c64b95999"
      },
      "env": {
        "EXPO_PUBLIC_PRODUCTION": "true",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID": "536289115131-tmonjjqnklakv93pc6mj969rmhsdmmvq.apps.googleusercontent.com",
        "EXPO_PUBLIC_Spotify_CLIENT_ID": "812c827d57b44b2497941ccb210ae022",
        "EXPO_PUBLIC_BACKEND_URL": "https://generate-a-website.fly.dev",
        "EXPO_PUBLIC_SVELTE_URL": "https://deployfirstwebsite-tofly-production.up.railway.app/",
        "EXPO_PUBLIC_VEXO_API_KEY": "9d8b2a88-06a0-4acf-9883-df17b45fde1b"
      }
    }
  }
}
